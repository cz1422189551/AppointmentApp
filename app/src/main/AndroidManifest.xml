<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cz.org.appointment">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/canguanyuyue"
        android:label="预约实验"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name=".activity.LoginActivity"
            android:launchMode="singleTask"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.HomeActivity"
            android:launchMode="singleTask">

        </activity>
        <activity
            android:name=".activity.RegisterActivity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".activity.MineActivity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".activity.AppointmentActivity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".activity.CommentActivity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".activity.LaboratoryInfoActivity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".activity.AnnouncementActivity"
            android:launchMode="singleTask"></activity>

    </application>

</manifest>